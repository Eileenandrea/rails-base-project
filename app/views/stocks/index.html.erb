<h1>Market Place</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Ticker</th>
      <th scope="col">Company Name</th>
      <th scope="col">Price</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <%=render @stocks%>
  </tbody>
</table>

<div class="flickr_pagination">
    <div class="page_info">
      <%= page_entries_info @stocks %>
    </div>
    <%= will_paginate @stocks, :container => false %>
  </div>

  